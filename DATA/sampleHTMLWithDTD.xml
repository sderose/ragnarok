<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html [
    <!-- The root element -->
    <!ELEMENT html   (head, body)>
    <!ELEMENT head   (title)>
    <!ELEMENT body   (div|p)*>

    <!ELEMENT div        (#PCDATA | p | ul | ol | dl | h1 | h2 | h3 | h4 | h5 | img)*>
    <!ELEMENT p          (#PCDATA | i | b | tt | u | sup | sub | abbr)*>
    <!ELEMENT ul         (li*)>
    <!ELEMENT ol         (li*)>
    <!ELEMENT li         (#PCDATA | i | b | tt | u | sup | sub | abbr)*>
    <!ELEMENT img        EMPTY>

    <!ELEMENT dl         (dt, dd)* >

    <!ATTLIST p
        lang      CDATA    'en'
        class     NMTOKENS #IMPLIED
        id        ID       #IMPLIED
        alt       CDATA    #IMPLIED
        greens    IDREFS   #IMPLIED
        ent       ENTITY   #IMPLIED
        ents      ENTITIES #IMPLIED
        nt        NMTOKEN  #IMPLIED

        egg       (foo)         "foo"
        eggs      (ban|bar|baz) "bar"

        monty     CDATA    #FIXED "python&#65;&lt;"
        >

    <!ATTLIST img
        id        ID       #IMPLIED
        href      CDATA    #REQUIRED
        alt       CDATA    #REQUIRED>

    <!NOTATION png   SYSTEM "pblort">
    <!NOTATION jpg   PUBLIC "-//jblort" "jblort">

    <!ENTITY spam      "eggs">
    <!ENTITY scene24   SYSTEM "zork.xml">
    <!ENTITY scene25   SYSTEM "zork.png" NDATA png>
]>
